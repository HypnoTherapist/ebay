<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        
        <menuitem id="menu_ebay_listing_activity" name="Listing Activity" sequence="60" parent="menu_ebay_top"/>
        
        <record model="ir.ui.view" id="ebay_item_form_view">
            <field name="name">ebay item form</field>
            <field name="model">ebay.item</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="ebay item form" version="7.0">
                    <header attrs="{'invisible': [('parent_id','!=',False)]}">
                        <button class="oe_highlight" name="action_verify" states="Draft,Completed,Ended" string="Verify" type="object" groups="ebay.group_ebay_manager"/>
                        <button class="oe_highlight" name="action_upload_picture" confirm='Are you sure you want to upload pictures of this item?' string="Upload Pictures" type="object" groups="ebay.group_ebay_manager"/>
                        <button class="oe_highlight" name="action_upload" confirm='Are you sure you want to upload this item?' states="Draft,Completed,Ended" string="Upload Item" type="object" groups="ebay.group_ebay_manager"/>
                        <button class="oe_highlight" name="action_revise" confirm='Are you sure you want to revise this item?' states="Active" string="Revise" type="object" groups="ebay.group_ebay_manager"/>
                        <button class="oe_highlight" name="action_synchronize" states="Active,Completed,Ended" string="Synchronize" type="object" groups="ebay.group_ebay_manager"/>
                        <button name="action_end_listing" confirm='Are you sure you want to end this item?' states="Active" string="End Listing" type="object" groups="ebay.group_ebay_manager"/>
                        <field name="state" widget="statusbar"/>
                    </header>
                    <sheet>
                        <field name="view_item_url" widget="url" attrs="{'invisible': [('item_id','=',False)]}"/>
                        <field name="parent_id" invisible="1"/>
                        <field name="variation_invalid" invisible="1"/>
                        <field name="variation_deleted" invisible="1"/>
                        <group attrs="{'invisible': [('parent_id','!=',False)]}">
                            <group>
                                <field name="site" attrs="{'readonly': [('state','!=','Draft')]}"/>
                            </group>
                            <group>
                                <field name="ebay_user_id" attrs="{'readonly': [('state','!=','Draft')]}"/>
                            </group>
                        </group>
                        <group>
                            <div>
                                <h2>
                                    <field name="name" default_focus="1" placeholder="Title"/>
                                </h2>
                                <field name="subtitle" placeholder="Subtitle" invisible="1"/>
                                <field name="primary_category_id" on_change="on_change_primary_category_id(primary_category_id, listing_type)" placeholder="Category" attrs="{'invisible': [('parent_id','!=',False)]}"/>
                                <field name="secondary_category_id" placeholder="2nd Category" invisible="1"/>
                                <div class="oe_title" attrs="{'invisible': [('parent_id','!=',False)]}">
                                    <field name="store_category_name" placeholder="Store Category" class="oe_title"/>
                                </div>
                                <div class="oe_title" attrs="{'invisible': [('parent_id','!=',False)]}">
                                    <field name="store_category2name" placeholder="2nd Store Category" class="oe_title"/>
                                </div>
                            </div>
                        </group>
                        <group>
                            <group string="Details" attrs="{'invisible': [('parent_id','!=',False)]}">
                                <field name="item_specifics"/>
                                <field name="condition_id"/>
                                <field name="condition_description_id"/>
                            </group>
                            <group string="Selling Format">
                                <field name="listing_type" on_change="on_change_listing_type(primary_category_id, listing_type)" attrs="{'readonly': [('state','!=','Draft')], 'invisible': [('parent_id','!=',False)]}"/>
                                <field name="start_price"/>
                                <field name="buy_it_now_price" attrs="{'invisible': [('listing_type','!=','Chinese')]}"/>
                                <field name="quantity" attrs="{'readonly': [('variation_deleted','=',True),('parent_id','!=',False)]}"/>
                                <field name="listing_duration" attrs="{'invisible': [('parent_id','!=',False)]}"/>
                            </group>
                        </group>
                        <group attrs="{'invisible': [('variation_invalid','=',True)]}">
                            <field name="variation" attrs="{'readonly': [('state','!=','Draft')]}"/>
                            <field name="variation_modify_specific_name" attrs="{'invisible': [('variation','=',False)]}"/>
                            <field name="variation_specific_name" attrs="{'invisible': [('variation','=',False)], 'required': [('variation','=',True)], 'readonly': [('state','=','Active')]}"/>
                            <field name="variation_specifics_set" attrs="{'invisible': [('variation','=',False)], 'required': [('variation','=',True)], 'readonly': [('parent_id','!=',False),('state','=','Active')]}"/>
                            <!-- variation view start -->
                            <field name="child_ids" attrs="{'invisible': [('variation','=',False)]}">
                                <tree>
                                    <field name="variation_specifics_set" string="Value"/>
                                    <field name="start_price"/>
                                    <field name="quantity"/>
                                    <field name="quantity_sold"/>
                                </tree>
                                <form string="Variation" version="7.0">
                                    <header>
                                        <field name="state" widget="statusbar"/>
                                    </header>
                                    <group>
                                        <field name="name" invisible="1"/>
                                        <field name="variation_specifics_set" string="Value" attrs="{'readonly': ['|',('variation_deleted','=',True),('state','=','Active')]}" on_change="on_change_variation_specifics_set(variation_specifics_set)"/>
                                        <field name="start_price" attrs="{'readonly': [('variation_deleted','=',True)]}"/>
                                        <field name="quantity" attrs="{'readonly': [('variation_deleted','=',True)]}"/>
                                        <field name="variation_deleted"/>
                                    </group>
                                    <group>
                                        <field name="eps_picture_ids" mode="kanban">
                                            <kanban>
                                                <field name="name"/>
                                                <field name="has_image"/>
                                                <field name="picture_format"/>
                                                <field name="use_by_date"/>
                                                <templates>
                                                    <t t-name="kanban-box">
                                                        <div class="oe_kanban_vignette">
                                                            <a t-if="! read_only_mode" type="delete" style="position: absolute; right: 0; padding: 4px; diplay: inline-block">Delete</a>
                                                            <a type="open">
                                                                <t t-if="record.has_image.raw_value === true and (!record.image or record.image.raw_value === false)">
                                                                    <img t-att-src="kanban_image('ebay.eps.picture', 'image', record.id.value, {'preview_image': 'image_small'})" class="oe_avatar oe_kanban_image"/>
                                                                </t>
                                                                <t t-if="record.image and record.image.raw_value !== false">
                                                                    <img t-att-src="'data:image/png;base64,'+record.image.raw_value" class="oe_avatar oe_kanban_image"/>
                                                                </t>
                                                            </a>
                                                            <div class="oe_kanban_details">
                                                                <h4 class="oe_partner_heading"><a type="open"><field name="name"/></a></h4>
                                                                <ul>
                                                                    <li t-if="record.picture_format.raw_value"><field name="picture_format"/></li>
                                                                    <li t-if="record.use_by_date.raw_value"><field name="use_by_date"/></li>
                                                                </ul>
                                                            </div>
                                                        </div>
                                                    </t>
                                                </templates>
                                            </kanban>
                                            <form string="EPS Picture" version="7.0">
                                                <sheet>
                                                    <group>
                                                        <field name="full_url" widget="url"/>
                                                        <field name="name" default_focus="1"/>
                                                    </group>
                                                    <field name="image" widget='image' class="oe_avatar oe_left" options='{"preview_image": "image", "size": [500, 500]}'/>
                                                </sheet>
                                            </form>
                                        </field>
                                    </group>
                                    <group>
                                        <field name="product_ids">
                                            <tree editable="bottom">
                                                <field name="product_id"/>
                                                <field name="uos_coeff" string="Coeff"/>
                                            </tree>
                                        </field>
                                    </group>
                                </form>
                            </field>
                            <!-- variation view end -->
                        </group>
                        <notebook colspan="4">
                            <page string="Pictures" autofocus="autofocus" style="min-height:480px">
                                <field name="eps_picture_ids" mode="kanban">
                                    <kanban>
                                        <field name="name"/>
                                        <field name="has_image"/>
                                        <field name="picture_format"/>
                                        <field name="use_by_date"/>
                                        <templates>
                                            <t t-name="kanban-box">
                                                <div class="oe_kanban_vignette">
                                                    <a t-if="! read_only_mode" type="delete" style="position: absolute; right: 0; padding: 4px; diplay: inline-block">Delete</a>
                                                    <a type="open">
                                                        <t t-if="record.has_image.raw_value === true and (!record.image or record.image.raw_value === false)">
                                                            <img t-att-src="kanban_image('ebay.eps.picture', 'image', record.id.value, {'preview_image': 'image_small'})" class="oe_avatar oe_kanban_image"/>
                                                        </t>
                                                        <t t-if="record.image and record.image.raw_value !== false">
                                                            <img t-att-src="'data:image/png;base64,'+record.image.raw_value" class="oe_avatar oe_kanban_image"/>
                                                        </t>
                                                    </a>
                                                    <div class="oe_kanban_details">
                                                        <h4 class="oe_partner_heading"><a type="open"><field name="name"/></a></h4>
                                                        <ul>
                                                            <li t-if="record.picture_format.raw_value"><field name="picture_format"/></li>
                                                            <li t-if="record.use_by_date.raw_value"><field name="use_by_date"/></li>
                                                        </ul>
                                                    </div>
                                                </div>
                                            </t>
                                        </templates>
                                    </kanban>
                                    <form string="EPS Picture" version="7.0">
                                        <sheet>
                                            <group>
                                                <field name="full_url" widget="url"/>
                                                <field name="name" default_focus="1"/>
                                            </group>
                                            <field name="image" widget='image' class="oe_avatar oe_left" options='{"preview_image": "image", "size": [500, 500]}'/>
                                            <!--
                                            <field name="picturesetmember_ids">
                                                <tree string="Picture Set Member">
                                                    <field name="member_url" widget="url"/>
                                                    <field name="picture_height"/>
                                                    <field name="picture_width"/>
                                                </tree>
                                            </field>
                                            -->
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                            <page string="Description" style="min-height:480px" attrs="{'invisible': [('parent_id','!=',False)]}">
                                <group>
                                    <field name="description_tmpl_id"/>
                                </group>
                                <field name="description" editor_height="480" style="margin-left:0px"/>
                            </page>
                            <page string="Product" style="min-height:480px" attrs="{'invisible': [('variation','=',True)]}">
                                <field name="product_ids" editable="bottom">
                                    <tree>
                                        <field name="product_id"/>
                                        <field name="uos_coeff" string="Coeff"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Shipping and Policies" style="min-height:480px" attrs="{'invisible': [('parent_id','!=',False)]}">
                                <group>
                                    <group>
                                        <field name="shipping_details_id"/>
                                    </group>
                                    <group>
                                        <field name="return_policy_id"/>
                                        <field name="buyer_requirement_details_id"/>
                                    </group>
                                </group>
                            </page>
                            <page string="Selling Status" style="min-height:480px" attrs="{'invisible': [('state','=','Draft')]}">
                                <group>
                                    <group>
                                        <field name="item_id"/>
                                        <field name="revise_date"/>
                                        <field name="start_time"/>
                                        <field name="hit_count"/>
                                        <field name="quantity_sold" attrs="{'invisible': [('listing_type','=','Chinese')]}"/>
                                        <field name="bid_count" attrs="{'invisible': [('listing_type','!=','Chinese')]}"/>
                                    </group>
                                    <group>
                                        <field name="time_left"/>
                                        <field name="update_date"/>
                                        <field name="end_time"/>
                                        <field name="watch_count"/>
                                    </group>
                                </group>
                            </page>
                            <page string="Errors" style="min-height:480px" attrs="{'invisible': [('parent_id','!=',False)]}">
                                <group>
                                    <group>
                                    <field name="severity_code_error"/>
                                    </group>
                                    <group>
                                        <field name="severity_code_warning"/>
                                    </group>
                                </group>
                                <field name="error_message"/>
                            </page>
                            <page string="Response" style="min-height:480px" attrs="{'invisible': [('parent_id','!=',False)]}">
                                <field name="response"/>
                            </page>
                        </notebook>
                        <group>
                            
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record model="ir.ui.view" id="ebay_item_tree_view">
            <field name="name">ebay item tree</field>
            <field name="model">ebay.item</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="ebay item tree">
                    <field name="need_to_be_updated" invisible="1"/>
                    <field name="severity_code_error" invisible="1"/>
                    <field name="severity_code_warning" invisible="1"/>
                    <button name="action_dummy" attrs="{'invisible': [('need_to_be_updated','=',False)]}" string="Edit" type="object" icon="gtk-edit" help=""/>
                    <button name="action_dummy" attrs="{'invisible': [('severity_code_error','=',False)]}" string="Error" type="object" icon="gtk-dialog-error" help=""/>
                    <button name="action_dummy" attrs="{'invisible': [('severity_code_warning','=',False)]}" string="Warning" type="object" icon="gtk-dialog-warning" help=""/>
                    <field name="name"/>
                    <field name="variation"/>
                    <field name="listing_type"/>
                    <field name="quantity"/>
                    <field name="listing_duration"/>
                    <field name="start_price"/>
                    <field name="state"/>
                    <!-- <field name="ebay_user_id"/> -->
                    <field name="revise_date"/>
                    <field name="update_date"/>
                </tree>
            </field>
        </record>
        
        <record id="view_ebay_item_filter" model="ir.ui.view">
            <field name="name">ebay item select</field>
            <field name="model">ebay.item</field>
            <field name="arch" type="xml">
                <search string="Search Item">
                    <filter icon="terp-document-new" string="Draft" domain="[('state','in',('Draft',))]"/>
                    <filter icon="terp-check" string="Active" domain="[('state','in',('Active',))]"/>
                    <filter icon="terp-dolar_ok!" string="Completed" domain="[('state','in',('Completed','Ended'))]"/>
                    <separator/>
                    <field name="name"/>
                    <group expand="0" string="Group By...">
                        <filter string="User" icon="terp-personal" domain="[]" context="{'group_by':'ebay_user_id'}"/>
                        <filter string="Format" icon="terp-stock_effects-object-colorize" domain="[]" context="{'group_by':'listing_type'}"/>
                        <filter string="Variation" icon="terp-stock_effects-object-colorize" domain="[]" context="{'group_by':'variation'}"/>
                        <filter string="Status" icon="terp-stock_effects-object-colorize" domain="[]" context="{'group_by':'state'}"/>
                    </group>
               </search>
            </field>
        </record>
        
        <record id="action_ebay_item_tree" model="ir.actions.act_window">
            <field name="name">Inventory</field>
            <field name="res_model">ebay.item</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('parent_id','=',False)]</field>
        </record>
        <menuitem id="ebay_menu_item" action="action_ebay_item_tree" sequence="20" parent="menu_ebay_listing_activity"/>
        
        <record model="ir.ui.view" id="ebay_product_form_view">
            <field name="name">ebay product form</field>
            <field name="model">ebay.product</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="ebay product form" version="7.0">
                    <group>
                        <field name="product_id"/>
                        <field name="uos_coeff"/>
                    </group>
                </form>
            </field>
        </record>
        
        <record model="ir.ui.view" id="ebay_details_form_view">
            <field name="name">ebay details form</field>
            <field name="model">ebay.details</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="ebay details form" version="7.0">
                    <header>
                        <button class="oe_highlight" name="action_update" string="Update eBay Details" type="object"/>
                    </header>
                    <sheet>
                        <div class="oe_right">
                            <field name="site_id" placeholder="Site"/>
                            <label for="sandbox"/>
                            <field name="sandbox" class="oe_inline"/>
                        </div>
                        <div class="oe_title">
                            <div class="oe_edit_only">
                                <label for="name"/>
                            </div>
                            <h1>
                                <field name="name" default_focus="1"/>
                            </h1>
                        </div>
                        <field name="ebay_details"/>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record model="ir.ui.view" id="ebay_details_tree_view">
            <field name="name">ebay details tree</field>
            <field name="model">ebay.details</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="ebay details tree">
                    <field name="name"/>
                    <field name="site_id"/>
                    <field name="sandbox"/>
                </tree>
            </field>
        </record>
        
        <record id="action_ebay_details_tree" model="ir.actions.act_window">
            <field name="name">eBay Details</field>
            <field name="res_model">ebay.details</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="ebay_menu_details" action="action_ebay_details_tree" sequence="60" parent="menu_ebay_listing_activity" groups="ebay.group_ebay_manager"/>
        
        <record model="ir.ui.view" id="ebay_category_form_view">
            <field name="name">ebay category form</field>
            <field name="model">ebay.category</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="ebay category form" version="7.0">
                    <header>
                        <button class="oe_highlight" name="action_update" string="Update Category Feature" type="object"/>
                    </header>
                    <sheet>
                        <div class="oe_right">
                            <field name="category_site_id" placeholder="Category Site"/>
                            <label for="sandbox"/>
                            <field name="sandbox" class="oe_inline"/>
                        </div>
                        <div class="oe_title">
                            <div class="oe_edit_only">
                                <label for="category_id"/>
                            </div>
                            <h1>
                                <field name="category_id" default_focus="1" placeholder="Category ID" />
                            </h1>
                        </div>
                        <field name="name"/>
                        <group>
                            <field name="condition_enabled"/>
                            <field name="condition_values"/>
                            <field name="free_gallery_plus_enabled"/>
                            <field name="free_picture_pack_enabled"/>
                            <field name="handling_time_enabled"/>
                            <field name="item_specifics_enabled"/>
                            <field name="variations_enabled"/>
                        </group>
                        <field name="category_feature"/>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record model="ir.ui.view" id="ebay_category_tree_view">
            <field name="name">ebay category tree</field>
            <field name="model">ebay.category</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="ebay category tree">
                    <field name="category_id"/>
                    <field name="name"/>
                    <field name="category_site_id"/>
                    <field name="sandbox"/>
                </tree>
            </field>
        </record>
        
        <record id="action_ebay_category_tree" model="ir.actions.act_window">
            <field name="name">eBay Categories</field>
            <field name="res_model">ebay.category</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="ebay_menu_category" action="action_ebay_category_tree" sequence="50" parent="menu_ebay_listing_activity" groups="ebay.group_ebay_manager"/>
        
        <record model="ir.ui.view" id="ebay_condition_description_form_view">
            <field name="name">ebay condition description form</field>
            <field name="model">ebay.conditiondescription</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="ebay condition description form" version="7.0">
                    <group>
                        <field name="name"/>
                        <field name="description"/>
                    </group>
                </form>
            </field>
        </record>
        
        <record model="ir.ui.view" id="ebay_condition_description_tree_view">
            <field name="name">ebay condition description tree</field>
            <field name="model">ebay.conditiondescription</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="ebay condition description tree">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="ebay_item_description_template_form_view">
            <field name="name">ebay item description template form</field>
            <field name="model">ebay.item.description.template</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="ebay item description template form" version="7.0">
                    <field name="name" placeholder='Template Name'/>
                    <field name="template"/>
                </form>
            </field>
        </record>
        
        <record model="ir.ui.view" id="ebay_item_description_template_tree_view">
            <field name="name">ebay item description template tree</field>
            <field name="model">ebay.item.description.template</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="ebay item description template tree">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="ebay_shipping_details_form_view">
            <field name="name">ebay shipping details form</field>
            <field name="model">ebay.shippingdetails</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="ebay shipping details form" version="7.0">
                    <sheet>
                        <div class="oe_title">
                            <div class="oe_edit_only">
                                <label for="name"/>
                            </div>
                            <h2>
                                <field name="name" default_focus="1"/>
                            </h2>
                        </div>
                        <field name="shipping_type" invisible='1'/>
                        <group>
                            <group string="Domestic Shipping">
                                <field name="sso_shipping_service"/>
                                <field name="sso_shipping_service_cost" attrs="{'readonly': [('sso_free_shipping','=',True)]}"/>
                                <field name="sso_shipping_service_additional_Cost" attrs="{'readonly': [('sso_free_shipping','=',True)]}"/>
                                <field name="sso_free_shipping" on_change="on_change_sso_free_shipping(sso_free_shipping)"/>
                            </group>
                            <group string="International Shipping">
                                <field name="isso_shipping_service"/>
                                <field name="isso_shipping_service_cost"/>
                                <field name="isso_shipping_service_additional_cost"/>
                            </group>
                        </group>
                        <label for="exclude_ship_to_location"/>
                        <field name="exclude_ship_to_location"/>
                    </sheet>
                </form>
            </field>
        </record>
        
        <record model="ir.ui.view" id="ebay_shipping_details_tree_view">
            <field name="name">ebay shipping details tree</field>
            <field name="model">ebay.shippingdetails</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="ebay shipping details tree">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        
        <record model="ir.ui.view" id="ebay_return_policy_form_view">
            <field name="name">ebay return policy form</field>
            <field name="model">ebay.returnpolicy</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="ebay return policy form" version="7.0">
                    <sheet>
                        <group>
                            <field name="name" default_focus="1"/>
                            <field name="returns_accepted_option"/>
