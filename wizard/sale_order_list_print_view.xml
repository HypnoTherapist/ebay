<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- wizard view -->
        <record model="ir.ui.view" id="sale_order_list_print_view">
            <field name="name">ebay sale order list print</field>
            <field name="model">ebay.sale.order.list.print</field>
            <field name="arch" type="xml">
                <form string="ebay sale order list print" version="7.0">
		    <separator string="Print Delivery Slip"/>
		    <group>
			<field name="count"/>
		    </group>
		    <footer>
			<button name="action_print" string="OK" type="object" class="oe_highlight"/>
			or
			<button string="Cancel" class="oe_link" special="cancel" />
		    </footer>
		</form>
	    </field>
	</record>

        <act_window id="sale_order_list_print"
            multi="True"
            key2="client_action_multi" name="Print Delivery Slip"
            res_model="ebay.sale.order.list.print" src_model="ebay.sale.order"
            view_mode="form" target="new" view_type="form" view_id="sale_order_list_print_view"/>

    </data>
</openerp>
